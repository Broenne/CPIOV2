uint16_t VirtAddVarTab[NumbOfVar] = { 0x0000, 0x0001, 0x0002, 0x003, 0x004, 0x005, 0x006 };


void InitVirtualEeprom(void) {
	__disable_irq();
	FLASH_Unlock();
	// EEPROM Init
	EE_Init();
	FLASH_Lock();
	delay_ms(2);
	__enable_irq();
}
